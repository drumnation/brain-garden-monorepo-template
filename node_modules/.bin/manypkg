#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules/@manypkg/cli/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules/@manypkg/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules/@manypkg/cli/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules/@manypkg/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/@manypkg+cli@0.21.4/node_modules:/Users/dmieloch/Dev/singularity-core/scheduling-station/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@manypkg/cli/bin.js" "$@"
else
  exec node  "$basedir/../@manypkg/cli/bin.js" "$@"
fi
